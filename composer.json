{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "digitalascetic/base-entity": "^4.0",
        "digitalascetic/query-filter": "^4.0",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.3.0",
        "symfony/console": "^4.3.0",
        "symfony/dotenv": "^4.3.0",
        "symfony/event-dispatcher": "^4.3.0",
        "symfony/expression-language": "^4.3.0",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^4.3.0",
        "symfony/framework-bundle": "^4.3.0",
        "symfony/http-client": "^4.3.0",
        "symfony/intl": "^4.3.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.3.0",
        "symfony/security-bundle": "^4.3.0",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^4.3.0",
        "symfony/twig-bundle": "^4.3.0",
        "symfony/validator": "^4.3.0",
        "symfony/web-link": "^4.3.0",
        "symfony/yaml": "^4.3.0",
        "twig/intl-extra": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.fury.io/8wbRFwcPzdATcTofLeQs/digitalascetic/"
        }
    ],
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "^4.3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^4.3.0"
        }
    }
}
